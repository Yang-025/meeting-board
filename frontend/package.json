  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server --port 3001 --hot --inline --env development",
    "start-remote": "webpack-dev-server --port 3001 --hot --inline --host 0.0.0.0 --env development",
    "build": "npm run clean && webpack -p --progress --colors --env production",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf coverage",
    "test": "jest --config=jest.config.json",
    "test:watch": "jest --config=jest.config.json --watch",
    "test:covrage": "jest --config=jest.config.json --coverage",
    "push": "git push --follow-tags origin master",
    "release": "standard-version",
    "release:first": "npm run release --sign --first-release",
    "release-patch": "npm run release -- --release-as patch",
    "release-minor": "npm run release -- --release-as minor",
    "release-major": "npm run release -- --release-as major",
    "commitmsg": "validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog:git:commit": "git add -A && git commit -m 'docs(CHANGELOG): update changelog'"
